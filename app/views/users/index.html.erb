<div class="page-header">
  <h1>
    Users
  </h1>
</div>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <ul class="list-group">
        <% @users.each do |user| %>
        <li class="list-group-item"><a href="/users/<%= user.id %>"><%= user.username %></a></li>
        <% end %>
      </ul>
    </table>
  </div>
</div>
